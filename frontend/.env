# API Configuration
# Dans frontend/.env
# API URL - Utilisation d'un chemin relatif pour le reverse proxy Nginx
REACT_APP_API_URL=/api/v1
NODE_ENV=development
GENERATE_SOURCEMAP=true
INLINE_RUNTIME_CHUNK=true

# Performance Optimization
DISABLE_ESLINT_PLUGIN=true
FAST_REFRESH=false

# Build Settings
BUILD_PATH=build

# Security Headers (will be set by Nginx)
# Disable source maps in production
WDS_SOCKET_PORT=0